sync:
	$(SYNCWEB) -lang ocaml option.nw option.ml
	$(SYNCWEB) -lang ocaml option.nw option.mli
	$(SYNCWEB) -lang ocaml main.nw main.ml


lpdistclean::
	rm -f $(LPSRC) .md5sum_*
