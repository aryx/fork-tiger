sync:
	$(SYNCWEB) -lang C runtime.nw runtime.c--
	$(SYNCWEB) -lang C runtime.nw client.c
	$(SYNCWEB) -lang C gc.nw gc.c
	$(SYNCWEB) -lang C gc.nw gc.h
	$(SYNCWEB) -lang C gc.nw alloc.c--


lpdistclean::
	rm -f $(LPSRC) .md5sum_*
