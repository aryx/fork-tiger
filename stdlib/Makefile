sync:
	$(SYNCWEB) -lang C stdlib.nw stdlib.h
	$(SYNCWEB) -lang C stdlib.nw stdlib.c
	$(SYNCWEB) -lang C stdlib.nw stdlibcmm.c--

lpdistclean::
	rm -f $(LPSRC) .md5sum_*
